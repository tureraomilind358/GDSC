{
  "name": "institute-management-frontend",
  "version": "1.0.0",
  "description": "Institute Management System - Angular Frontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/institute-management-frontend/stats.json",
    "prettier": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/service-worker": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.48.0",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "uuid": "^10.0.0",
    "js-cookie": "^3.0.5",
    "file-saver": "^2.0.5",
    "xlsx": "^0.18.5",
    "chart.js": "^4.4.0",
    "ng2-charts": "^5.0.4",
    "ngx-toastr": "^18.0.0",
    "ngx-spinner": "^18.0.0",
    "ngx-pagination": "^6.0.3",
    "ngx-skeleton-loader": "^8.1.0",
    "ngx-cookie-service": "^18.0.0",
    "ngx-webstorage": "^14.0.0",
    "ngx-permissions": "^18.0.0",
    "ngx-translate-messageformat-compiler": "^5.0.0",
    "ngx-translate-core": "^18.0.0",
    "ngx-translate-http-loader": "^9.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/file-saver": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~5.4.0",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "lint-staged": {
    "*.{ts,html,scss,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ]
}
